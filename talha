import.java.util.Arraylist;
import java.util.List;
import java.util.NoSuchElementException;
import java.util.Random;

public class ArrayListHeap{
private List<integer> a;
public ArrayListHeap() {
a=new Arraylist<>();
}
private void bubbleDown(int n) {
int miniChildIndex=miniChildIndex(n);
while(min ChildIndex!=-1 && a.get(minChildIndex)< a.get(n)) {
swap(minChildIndex,n);
n+minChildIndex;
minChildIndex=minChildIndex(n);
}
}
public boolean isHeap(){
for (int i=1;i<a.size(); ++1) {
if(par(i) >=0){
if (a.get(par(i))>a.get(i)) {
return false;
}
}
}
return true;
}
private void swap(int i,int j){
int tmp=a.get(i);
a.set(i,a.get(j));
a.set(j,tmp);
}
@override
public string tostring(){
return a.tostring();
}
}
